<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>FirstServelt</servlet-name>
		<display-name>FirstServelt</display-name>
		<description></description>
		<servlet-class>com.cai.servlet.FirstServelt</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SerializeServlet</servlet-name>
		<display-name>SerializeServlet</display-name>
		<description></description>
		<servlet-class>com.cai.servlet.SerializeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FirstServelt</servlet-name>
		<url-pattern>/FirstServelt</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SerializeServlet</servlet-name>
		<url-pattern>/SerializeServlet</url-pattern>
	</servlet-mapping>
</web-app>
